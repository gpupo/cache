{
    "name": "gpupo/cache",
    "description": "Caching library that implements PSR-6",
    "homepage": "https://opensource.gpupo.com/cache/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["cache", "psr-6"],
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "gpupo/common": "*",
        "psr/cache": "^1.0",
        "fig/cache-util": "dev-master as 1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\Cache\\": "tests",
            "Gpupo\\Example\\": "src/Example"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
