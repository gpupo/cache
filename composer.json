{
    "name": "gpupo/cache",
    "description": "Caching library that implements PSR-6",
    "keywords": ["cache", "psr-6"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "gpupo/common": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "provide": {
        "psr/cache-implementation": "1.0.0"
    },
    "replace": {
        "psr/cache": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\Cache\\": "src",
            "Psr\\": "src/Psr/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\Cache\\": "tests",
            "Gpupo\\Example\\": "src/Example"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "minimum-stability" : "stable"
}
