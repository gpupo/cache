{
    "name": "gpupo/cache",
    "description": "Caching library that implements PSR-6",
    "keywords": ["cache", "psr-6"],
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "gpupo/common": "*",
        "psr/cache": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\Cache\\": "tests",
            "Gpupo\\Example\\": "src/Example"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
