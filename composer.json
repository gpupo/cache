{
    "name": "gpupo/cache",
    "description": "Caching library that implements PSR-6",
    "keywords": ["cache", "psr-6"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "gpupo/common": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "provide": {
        "psr/cache-implementation": "1.0.0"
    },
    "replace": {
        "psr/cache": "1.0.0"
    },    
    "autoload": {
        "psr-4": { "Gpupo\\Cache\\": "src/Gpupo/Cache/", "Psr\\": "src/Psr/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\": "tests/Gpupo/Tests",
            "Gpupo\\Example\\": "src/Gpupo/Example"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability" : "dev"
}
